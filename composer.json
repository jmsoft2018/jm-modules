{
  "name": "jmsoft/jm-modules",
  "description": "Package for using module structure in Laravel",
  "type": "library",
  "keywords": [
    "laravel",
    "module"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Javlon Annamuratov",
      "email": "annamuratov1987@gmail.com",
      "homepage": "https://github.com/jmsoft2018/jm-modules",
      "role": "Developer"
    }
  ],
  "support": {
    "source": "https://github.com/jmsoft2018/jm-modules"
  },
  "require": {
    "php": ">=7.0.0"
  },
  "autoload": {
    "psr-4": {
      "JmModules\\": "src",
      "JmModules\\": "../../../jm-modules"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "JmModules\\ModuleServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "php -r \"copy('jm-modules/config.php', 'jm-modules/config.php');\""
    ]
  }
}